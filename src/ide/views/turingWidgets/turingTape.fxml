<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" onKeyPressed="#moveHead" fx:controller="ide.views.turingWidgets.TuringWidgetController">
   <children>
      <VBox onKeyPressed="#moveHead" alignment="CENTER" prefHeight="150.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addCellLeft" onKeyPressed="#moveHead" prefHeight="20.0" prefWidth="50.0" text="+">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteCellLeft" onKeyPressed="#moveHead" prefHeight="20.0" prefWidth="50.0" text="-">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <ScrollPane onKeyPressed="#moveHead" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="300.0" vbarPolicy="NEVER">
         <content>
            <HBox fx:id="cellContainer" onKeyPressed="#moveHead" alignment="CENTER" prefHeight="148.0" prefWidth="200.0" spacing="5.0">
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
            </HBox>
         </content>
      </ScrollPane>
      <VBox onKeyPressed="#moveHead" alignment="CENTER" prefHeight="150.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addCellRight" onKeyPressed="#moveHead" prefHeight="20.0" prefWidth="50.0" text="+">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#deleteCellRight" onKeyPressed="#moveHead" prefHeight="20.0" prefWidth="50.0" text="-">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</HBox>
